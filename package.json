{
  "name": "cotiza-studio",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "help": "node scripts/index.js",
    "setup": "node scripts/setup.js",
    "dev": "node scripts/dev-start.js",
    "dev:raw": "turbo run dev",
    "health": "node scripts/health-check.js",
    "build": "turbo run build",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch",
    "test:cov": "turbo run test:cov",
    "test:e2e": "turbo run test:e2e",
    "lint": "turbo run lint",
    "clean": "turbo run clean",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "db:generate": "turbo run db:generate",
    "db:push": "turbo run db:push",
    "db:migrate": "turbo run db:migrate",
    "db:seed": "turbo run db:seed",
    "db:utils": "node scripts/db-utils.js",
    "implementation:status": "node scripts/implementation-tracker.js progress",
    "implementation:list": "node scripts/implementation-tracker.js list",
    "implementation:complete": "node scripts/implementation-tracker.js complete",
    "implementation:note": "node scripts/implementation-tracker.js note",
    "implementation:report": "node scripts/implementation-tracker.js report",
    "postinstall": "npm run db:generate"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "tsup": "^8.0.2",
    "turbo": "2.6.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.15.0"
}
